<UserControl x:Class="Fotowoltaika_Aplikacja.Views.Pokaz_wyniki"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Fotowoltaika_Aplikacja.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <DataGrid Margin="35,92,35,52"/>
        <Label Content="Tabela wyników" HorizontalAlignment="Left" Height="35" Margin="282,20,0,0" VerticalAlignment="Top" Width="173" FontSize="18" RenderTransformOrigin="0.5,0.584"/>
        <Button Content="{Binding Zaladujplik}" HorizontalAlignment="Left" Height="23" Margin="460,47,0,0" VerticalAlignment="Top" Width="120" Click="Zaladuj_Plik_Button"/>
        <Button Content="{Binding StworzWykres}" HorizontalAlignment="Left" Height="23" Margin="660,47,0,0" VerticalAlignment="Top" Width="120" Click="Stworz_Wykres_Button" />
        <Button Content="{Binding Powrot}" HorizontalAlignment="Left" Height="23" Margin="100,47,0,0" VerticalAlignment="Top" Width="120" Click="Powrot2_Button"/>
        <CheckBox Content="CheckBox" Margin="780,153,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.524,0.678"/>

    </Grid>
</UserControl>
